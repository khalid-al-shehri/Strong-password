<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.kh09909.strongpassword.RandomPage"
    android:background="@drawable/firstpage"
    >

    <com.google.android.gms.ads.AdView xmlns:ads="http://schemas.android.com/apk/res-auto"
        android:id="@+id/adView"
        android:layout_width="345dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="628dp"
        ads:adSize="BANNER"
        ads:adUnitId="ca-app-pub-9554171393741373/6340763489"
        ads:layout_constraintEnd_toEndOf="parent"
        ads:layout_constraintStart_toStartOf="parent"
        ads:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

    </com.google.android.gms.ads.AdView>


    <TextView
        android:id="@+id/tv2"
        android:layout_width="130dp"
        android:layout_height="29dp"
        android:layout_marginEnd="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="96dp"
        android:shadowColor="@android:drawable/screen_background_light"
        android:textColor="@android:drawable/screen_background_light"
        android:textColorHighlight="@android:drawable/screen_background_light"
        android:textColorHint="@android:drawable/screen_background_light"
        android:textColorLink="@android:drawable/screen_background_light"
        app:layout_constraintEnd_toEndOf="@+id/tv1"
        app:layout_constraintHorizontal_bias="0.702"
        app:layout_constraintStart_toEndOf="@+id/cobybtn"
        app:layout_constraintTop_toBottomOf="@+id/tv1" />

    <CheckBox
        android:id="@+id/cb1"
        android:layout_width="106dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/cb2"
        android:layout_alignBottom="@+id/cb2"
        android:layout_alignStart="@+id/cb3"
        android:layout_marginEnd="96dp"
        android:layout_marginTop="144dp"
        android:shadowColor="@android:drawable/screen_background_light"
        android:text="حروف كبيرة"
        android:textColor="@android:drawable/screen_background_light"
        android:textColorHighlight="@android:drawable/screen_background_light"
        android:textColorHint="@android:drawable/screen_background_light"
        android:textColorLink="@android:drawable/screen_background_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"

        />

    <CheckBox
        android:id="@+id/cb2"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/cb4"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="24dp"
        android:shadowColor="@android:drawable/screen_background_light"
        android:text="حروف صغيرة"
        android:textColor="@android:drawable/screen_background_light"
        android:textColorHighlight="@android:drawable/screen_background_light"
        android:textColorHint="@android:drawable/screen_background_light"
        android:textColorLink="@android:drawable/screen_background_light"
        app:layout_constraintEnd_toEndOf="@+id/cb1"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="@+id/cb1"
        app:layout_constraintTop_toBottomOf="@+id/cb1"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cb3"
        android:layout_width="78dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/cb4"
        android:layout_alignBottom="@+id/cb4"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="24dp"
        android:shadowColor="@android:color/white"
        android:text="ارقام"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/white"
        android:textColorHint="@android:color/white"
        android:textColorLink="@android:color/background_light"
        app:layout_constraintEnd_toStartOf="@+id/cb2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/cb4"
        app:layout_constraintTop_toBottomOf="@+id/cb4"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/cb4"
        android:layout_width="62dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cb2"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="144dp"
        android:shadowColor="@android:drawable/screen_background_light"
        android:text="رموز"
        android:textColor="@android:drawable/screen_background_light"
        android:textColorHighlight="@android:drawable/screen_background_light"
        android:textColorHint="@android:drawable/screen_background_light"
        android:textColorLink="@android:color/background_light"
        app:layout_constraintEnd_toStartOf="@+id/cb1"
        app:layout_constraintHorizontal_bias="0.877"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv1"
        android:layout_width="302dp"
        android:layout_height="27dp"
        android:layout_below="@+id/et1En"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="44dp"
        android:shadowColor="@android:drawable/screen_background_light"
        android:textAlignment="center"
        android:textColor="@android:drawable/screen_background_light"
        android:textColorHighlight="@android:drawable/screen_background_light"
        android:textColorHint="@android:drawable/screen_background_light"
        android:textColorLink="@android:drawable/screen_background_light"
        app:layout_constraintEnd_toEndOf="@+id/okbtn"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="@+id/et1En"
        app:layout_constraintTop_toBottomOf="@+id/et1En"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/okbtn"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/et1En"
        android:layout_alignBottom="@+id/et1En"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/okbutton"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/cb2"
        app:layout_constraintTop_toBottomOf="@+id/cb2"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/cobybtn"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="96dp"
        android:background="@drawable/copybutton"
        android:text=""
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/tv1"
        app:layout_constraintTop_toBottomOf="@+id/tv1"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/et1En"
        android:layout_width="120dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/cb3"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="44dp"
        android:ems="10"
        android:hint="أدخل طول كلمة السر"
        android:inputType="number"
        android:shadowColor="@android:drawable/screen_background_light"
        android:textColor="@android:drawable/screen_background_light"
        android:textColorHighlight="@android:drawable/screen_background_light"
        android:textColorHint="@android:drawable/screen_background_light"
        android:textColorLink="@android:drawable/screen_background_light"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/okbtn"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/cb3"
        app:layout_constraintTop_toBottomOf="@+id/cb3" />

</android.support.constraint.ConstraintLayout>
